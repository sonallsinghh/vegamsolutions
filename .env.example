# Copy to .env and fill in your values (do not commit .env).

# Milvus Cloud (Zilliz)
MILVUS_URI=
MILVUS_TOKEN=

# OpenAI (agent LLM). When set, the agent uses OpenAI instead of Hugging Face.
OPENAI_API_KEY=
# Optional: model for agent (default gpt-4o-mini). Examples: gpt-4o-mini, gpt-4o, gpt-4-turbo
# OPENAI_LLM_MODEL=gpt-4o-mini

# Hugging Face (embeddings; agent LLM fallback when OPENAI_API_KEY is not set)
HF_API_KEY=
# Optional: HF LLM for agent (only if OPENAI_API_KEY is not set)
# HF_LLM_MODEL=meta-llama/Llama-3.2-3B-Instruct
